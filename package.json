{
  "name": "client-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx tsc && npx vite build",
    "lint": "npx eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "npx vite preview",
    "scaffold": "npx swagger-typescript-api -p http://localhost:8080/swagger/v1/swagger.json -o ./ -n Api.ts --axios",
    "deploy": "npx firebase deploy --only hosting",
    "diagram": "npx depcruise . --exclude \"^node_modules\" --output-type dot | dot -T svg > dependency-graph.svg"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@faker-js/faker": "^8.4.1",
    "babel-plugin-react-compiler": "0.0.0-experimental-938cd9a-20240601",
    "daisyui": "^4.12.10",
    "eslint-plugin-react-compiler": "0.0.0-experimental-51a85ea-20240601",
    "jotai": "^2.9.1",
    "jotai-devtools": "^0.10.0",
    "jotai-effect": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.25.1",
    "theme-change": "^2.5.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7",
    "@typescript-eslint/parser": "^7",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "dependency-cruiser": "^16.3.10",
    "eslint": "^8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "firebase-tools": "^13.14.1",
    "jsonwebtoken": "^9.0.2",
    "postcss": "^8.4.40",
    "swagger-typescript-api": "^13.0.13",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.4",
    "vite": "^5.3.4",
    "vite-plugin-singlefile": "^2.0.2"
  }
